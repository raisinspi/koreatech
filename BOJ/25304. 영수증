#include <stdio.h>

int main(void) {
    int i, a, b, c, d;
    int total=0;
    scanf("%d\n%d", &a, &b);
    
    for(i=0; i<b; i++) {
        scanf("%d %d\n", &c, &d);
        total += c*d;
    }
    
    if(total==a) {
        printf("Yes");
    }
    else {
        printf("No");
    }
    
    return 0;
}
