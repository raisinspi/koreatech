#include <stdio.h>
#include <string.h>

int main(void) {
    char s[20];
    int n, a;
    
    scanf("%d", &n);
    for(int i=0; i<n; i++) {
        scanf("%d %s", &a, s);
        for(int j=0; j<strlen(s); j++) {
            for(int k=0; k<a; k++) {
                printf("%c", s[j]);
            }
        }
        printf("\n");
    }
    
    return 0;
}
